<script type="text/javascript"> 
var managePage = "/groupware/facility/admin/conferenceRoomManage.do";

(function($) {
	$(document).ready(function(){
		$("#viewDiv").loading();
		$("#viewDiv").load("/groupware/facility/admin/facilityList.do?categoryId=FC0000000", function() {
			$("#viewDiv").loaded();
		});
		
		$("a", "#facilityList").click(function( event) {
			event.preventDefault();
			
			$("#viewDiv").loading();
			$("#viewDiv").load("/groupware/facility/admin/facilityView.do?facilityId=" + $(this).attr("data-facilityId"), function() {
				$("#viewDiv").loaded();
			});
		});
		
		$("button.btnNew", "#facilityListDiv").click(function() {
			$("#viewDiv").loading();
			$("#viewDiv").load("/groupware/facility/admin/facilityForm.do?categoryId=FC0000000", function() {
				$("#viewDiv").loaded();
			});
		});
	});
})(jQuery);
</script>
 
